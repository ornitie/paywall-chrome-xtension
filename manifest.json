{
  "manifest_version": 2,
  "name": "Paywall Chrome Xtension",
  "version": "0.2.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.semana.com/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "domManipulation.js",
    "functions.js"
  ]
}