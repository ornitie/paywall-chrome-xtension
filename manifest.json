{
  "manifest_version": 2,
  "name": "Paywall Chrome Xtension",
  "version": "0.2.0",
  "description": "A test for webrequest",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.semana.com/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "domManipulation.js",
    "functions.js"
  ],
  "background": {
    "scripts": [
      "eltiempo.js"
    ],
    "persistent": true
  }
}